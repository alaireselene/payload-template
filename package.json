{
  "name": "bke-srn",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "next dev",
    "dev:prod": "rm -rf .next && bun run build && bun run start",
    "generate:importmap": "payload generate:importmap",
    "generate:types": "payload generate:types",
    "lint": "biome lint",
    "check": "biome check --write",
    "payload": "payload",
    "reinstall": "rm -rf node_modules && rm bun.lock && bun --ignore-workspace install",
    "start": "next start",
    "test": "bun test:int && bun test:e2e",
    "test:e2e": "bun playwright test --config=playwright.config.ts",
    "test:int": "vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@payloadcms/admin-bar": "3.69.0",
    "@payloadcms/db-postgres": "3.69.0",
    "@payloadcms/live-preview-react": "3.69.0",
    "@payloadcms/next": "3.69.0",
    "@payloadcms/plugin-form-builder": "3.69.0",
    "@payloadcms/plugin-nested-docs": "3.69.0",
    "@payloadcms/plugin-redirects": "3.69.0",
    "@payloadcms/plugin-search": "3.69.0",
    "@payloadcms/plugin-seo": "3.69.0",
    "@payloadcms/richtext-lexical": "3.69.0",
    "@payloadcms/ui": "3.69.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "payload": "3.69.0",
    "prism-react-renderer": "^2.3.1",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.69.0",
    "shadcn": "^3.6.2",
    "sharp": "0.34.5",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@eslint/eslintrc": "^3.2.0",
    "@playwright/test": "1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/react": "16.3.1",
    "@types/escape-html": "^1.0.2",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "16.1.1",
    "jsdom": "27.4.0",
    "playwright": "1.57.0",
    "playwright-core": "1.57.0",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.1.18",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "4.0.16"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["sharp", "esbuild", "unrs-resolver"]
  },
  "overrides": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  }
}
